pipeline {
    agent any

    stages {
        stage('test-pipeline') {
            steps {
                echo "test pipeline"
            }
        }
        stage('create Amazon ECR repository'){
			steps{
				echo "Inside creation repo"
				sh 'aws ecr create-repository --repository-name devops-nxt-repo'
				echo "Successfully created Amazon ECR"
			}        
        }
     }  
}